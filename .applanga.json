{
  "app": {
    "base_language": "en",
    "push": {
      "source": [
        {
          "path": "./common.json",
          "file_format": "nested_json",
          "tag": "JSON1",
          "language": "en"
        },
        {
          "path": "./createusers.json",
          "file_format": "nested_json",
          "tag": "JSON2",
          "language": "en"
        },
        {
          "path": "./profile.properties",
          "file_format": "java_properties",
          "tag": "Prop1",
          "language": "en"
        },
        {
          "path": "./rolesAndPrivileges.csv",
          "file_format": "csv",
          "tag": "CSV1",
          "columnDescription": {
            "KEY": 0,
            "en": 1
          },
          "excludeHeaderRow": true,
          "language": "en"
        }
      ]
    },
    "pull": {
      "target": [
        {
          "path": "./<language>.common.json",
          "file_format": "nested_json",
          "tag": "JSON1",
          "exclude_languages": [
            "en"
          ]
        },
        {
          "path": "./<language>.createusers.json",
          "file_format": "nested_json",
          "tag": "JSON2",
          "exclude_languages": [
            "en"
          ]
        },
        {
          "path": "./<language>.profile.properties",
          "file_format": "java_properties",
          "tag": "Prop1",
          "exclude_languages": [
            "en"
          ]
        },
        {
          "path": "./<language>.rolesAndPrivileges.csv",
          "file_format": "csv",
          "tag": "CSV1",
          "excludeHeaderRow": true,
          "excludeBaseLang": true,
          "exclude_languages": [
            "en"
          ]
        }
      ]
    }
  }
}