{
  "app": {
    "base_language": "en",
    "push": {
      "source": [
        {
          "path": "./common.json",
          "file_format": "nested_json",
          "tag": "JSON1",
          "language": "en"
        },
        {
          "path": "./createusers.json",
          "file_format": "react_nested_json",
          "tag": "JSON2",
          "language": "en"
        },
        {
          "path": "./profile.properties",
          "file_format": "java_properties",
          "tag": "Prop1",
          "language": "en"
        },
        {
          "path": "./rolesAndPrivileges.csv",
          "file_format": "csv",
          "tag": "CSV1",
          "columnDescription": {
            "KEY": 0,
            "en": 1
          },
          "excludeHeaderRow": false,
          "language": "en"
        },
        {
          "file_format": "csv",
          "path": "./Countries_en.csv",
          "tag": "CSV2",
          "key_prefix": "Countries-",
          "excludeHeaderRow": false,
          "columnDescription": {
            "KEY": 0,
            "en": 1
          },
          "language": "en"
        }
	
      ]
    },
    "pull": {
      "target": [
        {
          "path": "./<language>.common.json",
          "file_format": "nested_json",
          "tag": "JSON1",
          "exclude_languages": [
            "en"
          ]
        },
        {
          "path": "./Countries_<language>.csv",
          "file_format": "csv",
          "tag": "CSV2",
          "excludeHeaderRow": true,
          "excludeBaseLang": true,
	 "columnDescription": {
            "KEY": 0,
            "es": 1
          },	
          "exclude_languages": [
            "en"
          ]
        },
        {
          "path": "./<language>.createuser1.json",
          "file_format": "react_nested_json",
          "tag": "JSON2",
          "exclude_languages": [
            "en"
          ]
        },
        {
          "path": "./<language>.profile.properties",
          "file_format": "java_properties",
          "tag": "Prop1",
          "exclude_languages": [
            "en"
          ]
        },
        {
          "path": "./<language>.rolesAndPrivileges.csv",
          "file_format": "csv",
          "tag": "CSV1",
          "excludeHeaderRow": true,
          "excludeBaseLang": true,
		 "columnDescription": {
            "KEY": 0,
            "es": 1
          },
          "exclude_languages": [
            "en"
          ]
        }
      ]
    }
  },
  "languageMap": {
		"zh-HK": "zh_HK"
	}
}
